{
  "name": "zero-to-production",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "commit": "npx git-cz",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "e2e:chrome": "testcafe -c 3 chrome apps/todos/todos-web/todos-e2e/",
    "snapshot:update": "jest --updateSnapshot --testPathPattern",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format:fix": "pretty-quick --staged",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "lint-staged": "lint-staged",
    "build:scripts": "tsc -p tools/scripts/tsconfig.json",
    "validate-packages": "node ./tools/scripts/bin/validate-packages.js",
    "nx": "nx",
    "dev:ssr": "ng run examples-web:serve-ssr",
    "serve:ssr": "node dist/examples-web/server/main.js",
    "build:ssr": "ng build examples-web --prod && ng run examples-web:server:production",
    "prerender": "ng run examples-web:prerender"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.json": [
      "npm run validate-packages",
      "npm run format:fix"
    ],
    "*.ts": [
      "npm run format:fix"
    ]
  },
