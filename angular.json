{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "frontends-todos-todos": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/frontends/todos/todos",
      "sourceRoot": "apps/frontends/todos/todos/src",
      "prefix": "todo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/frontends/todos/todos",
            "index": "apps/frontends/todos/todos/src/index.html",
            "main": "apps/frontends/todos/todos/src/main.ts",
            "polyfills": "apps/frontends/todos/todos/src/polyfills.ts",
            "tsConfig": "apps/frontends/todos/todos/tsconfig.app.json",
            "assets": [
              "apps/frontends/todos/todos/src/favicon.ico",
              "apps/frontends/todos/todos/src/assets",
              "apps/frontends/todos/todos/src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/frontends/todos/todos/src/styles.scss"
            ],
            "scripts": [],
            "aot": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/frontends/todos/todos/src/environments/environment.ts",
                  "with": "apps/frontends/todos/todos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "apps/frontends/todos/todos/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "frontends-todos-todos:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontends-todos-todos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "frontends-todos-todos:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/frontends/todos/todos/tsconfig.app.json",
              "apps/frontends/todos/todos/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/frontends/todos/todos/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/frontends/todos/todos/jest.config.js",
            "tsConfig": "apps/frontends/todos/todos/tsconfig.spec.json",
            "setupFile": "apps/frontends/todos/todos/src/test-setup.ts"
          }
        }
      }
    },
    "frontends-todos-todos-e2e": {
