{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "frontends-todos-todos": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/frontends/todos/todos",
      "sourceRoot": "apps/frontends/todos/todos/src",
      "prefix": "workspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/frontends/todos/todos",
            "index": "apps/frontends/todos/todos/src/index.html",
            "main": "apps/frontends/todos/todos/src/main.ts",
            "polyfills": "apps/frontends/todos/todos/src/polyfills.ts",
            "tsConfig": "apps/frontends/todos/todos/tsconfig.app.json",
            "assets": [
              "apps/frontends/todos/todos/src/favicon.ico",
              "apps/frontends/todos/todos/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/frontends/todos/todos/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/frontends/todos/todos/src/environments/environment.ts",
                  "with": "apps/frontends/todos/todos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "frontends-todos-todos:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontends-todos-todos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "frontends-todos-todos:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/frontends/todos/todos/tsconfig.app.json",
              "apps/frontends/todos/todos/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/frontends/todos/todos/jest.config.js",
            "tsConfig": "apps/frontends/todos/todos/tsconfig.spec.json",
            "setupFile": "apps/frontends/todos/todos/src/test-setup.ts"
          }
        }
      }
    },
    "frontends/todos-todos-e2e": {
      "root": "apps/frontends/todos/todos-e2e",
      "sourceRoot": "apps/frontends/todos/todos-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/frontends/todos/todos-e2e/cypress.json",
            "tsConfig": "apps/frontends/todos/todos-e2e/tsconfig.e2e.json",
            "devServerTarget": "frontends-todos-todos:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontends-todos-todos:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/frontends/todos/todos-e2e/tsconfig.e2e.json"
          }
        }
      }
    },
    "common-ui-side-nav": {
      "projectType": "library",
      "root": "libs/frontend/common/ui/side-nav",
      "sourceRoot": "libs/frontend/common/ui/side-nav/src",
      "prefix": "todos",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/frontend/common/ui/side-nav/tsconfig.lib.json",
              "libs/frontend/common/ui/side-nav/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/frontend/common/ui/side-nav/jest.config.js",
            "tsConfig": "libs/frontend/common/ui/side-nav/tsconfig.spec.json",
            "setupFile": "libs/frontend/common/ui/side-nav/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "frontends-todos-todos"
}
